#include <iostream>
using namespace std;

// Function to count trailing zeroes in factorial of x
int countTrailingZeroes(int x) {
    int count = 0;
    while (x >= 5) {
        count += x / 5;
        x /= 5;
    }
    return count;
}

// Function to find the smallest number with at least n trailing zeroes in factorial
int findNumberWithTrailingZeroes(int n) {
    int low = 0, high = 5 * n, result = -1;
    while (low <= high) {
        int mid = (low + high) / 2;
        int zeroes = countTrailingZeroes(mid);
        if (zeroes >= n) {
            result = mid;
            high = mid - 1; // try to find smaller value
        } else {
            low = mid + 1;
        }
    }
    return result;
}

int main() {
    int n;
    cout << "Enter n: ";
    cin >> n;
    int ans = findNumberWithTrailingZeroes(n);
    cout << "Smallest number whose factorial has at least " << n << " trailing zeroes: " << ans << endl;
    return 0;
}
